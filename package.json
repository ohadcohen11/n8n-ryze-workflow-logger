{
	"name": "n8n-nodes-ryze-workflow-logger",
	"version": "0.1.0",
	"description": "Custom n8n node for logging workflow execution metrics to MySQL database",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ryze",
		"workflow",
		"logger",
		"mysql"
	],
	"author": {
		"name": "Ryze Beyond",
		"email": "ohad.cohen@ryzebeyond.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ohadcohen11/n8n-ryze-workflow-logger.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "echo 'Skipping lint'",
		"lint:fix": "echo 'Skipping lint'",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/GithubIssuesApi.credentials.js",
			"dist/credentials/GithubIssuesOAuth2Api.credentials.js",
			"dist/credentials/MySqlApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/GithubIssues/GithubIssues.node.js",
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/WorkflowLogger/WorkflowLogger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"dependencies": {
		"mysql2": "^3.11.5"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
